<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAEEE0"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="476dp"
        android:layout_height="99dp"
        android:background="#9A240A"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="445dp"
        android:layout_height="89dp"
        android:background="#9A240A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="108dp"
        android:layout_height="80dp"
        android:layout_marginBottom="12dp"
        android:width="15dp"
        android:gravity="center_horizontal|top"
        app:backgroundTint="@android:color/transparent"
        app:elevation="0dp"
        app:icon="@drawable/baseline_home_20"
        app:iconGravity="textEnd"
        app:iconPadding="0dp"
        app:iconSize="84dp"
        app:iconTint="#f2ac34"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:strokeColor="#FFFFFF" />

    <Button

        android:id="@+id/eventos"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="107dp"
        android:layout_height="88dp"
        android:layout_marginBottom="4dp"
        app:backgroundTint="@android:color/transparent"
        app:icon="@drawable/baseline_insert_invitation_18"
        app:iconGravity="textEnd"
        app:iconPadding="0dp"
        app:iconSize="74dp"
        app:iconTint="#f2ac34"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.548"
        app:layout_constraintStart_toEndOf="@+id/button4" />

    <Button
        android:id="@+id/perfil"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@android:color/transparent"
        app:icon="@drawable/baseline_account_circle_18"
        app:iconPadding="0dp"
        app:iconSize="72dp"
        app:iconTint="#f2ac34"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintHorizontal_bias="0.614"
        app:layout_constraintStart_toStartOf="@+id/textView2" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="79dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pipoca" />

    <ImageView
        android:id="@+id/TheFlash"
        android:layout_width="128dp"
        android:layout_height="192dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/WandaVision"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintVertical_bias="0.238"
        app:srcCompat="@drawable/the_flash_solo_poster_print__24_x_36_" />

    <ImageView
        android:id="@+id/WandaVision"
        android:layout_width="149dp"
        android:layout_height="192dp"
        android:padding="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238"
        app:srcCompat="@drawable/wandavision__trailer___poster" />

    <ImageView
        android:id="@+id/Anne"
        android:layout_width="154dp"
        android:layout_height="192dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238"
        app:srcCompat="@drawable/anne_poster" />

    <EditText
        android:id="@+id/editPesquisa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:layout_marginRight="48dp"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Pesquisa"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.555" />

</androidx.constraintlayout.widget.ConstraintLayout>