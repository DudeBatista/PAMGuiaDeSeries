<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAEEE0"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="476dp"
        android:layout_height="99dp"
        android:background="#9A240A"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="445dp"
        android:layout_height="89dp"
        android:background="#9A240A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button

        android:id="@+id/eventos"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="107dp"
        android:layout_height="88dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        app:backgroundTint="@android:color/transparent"
        app:icon="@drawable/baseline_insert_invitation_18"
        app:iconGravity="textEnd"
        app:iconPadding="0dp"
        app:iconSize="74dp"
        app:iconTint="#f2ac34"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toEndOf="@+id/perfil" />

    <Button
        android:id="@+id/perfil"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        app:backgroundTint="@android:color/transparent"
        app:icon="@drawable/baseline_account_circle_18"
        app:iconPadding="0dp"
        app:iconSize="72dp"
        app:iconTint="#f2ac34"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView2" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="79dp"
        android:layout_height="83dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pipoca" />

    <ImageView
        android:id="@+id/TheFlash"
        android:layout_width="120dp"
        android:layout_height="208dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:contentDescription="@string/the_flash"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintVertical_bias="0.225"
        app:srcCompat="@drawable/the_flash_solo_poster_print__24_x_36_" />

    <ImageView
        android:id="@+id/WandaVision"
        android:layout_width="121dp"
        android:layout_height="181dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:contentDescription="@string/wanda"
        android:onClick="abrirSerie"
        android:padding="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/TheFlash"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238"
        app:srcCompat="@drawable/wandavision__trailer___poster" />

    <ImageView
        android:id="@+id/Anne"
        android:layout_width="121dp"
        android:layout_height="181dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:contentDescription="@string/anne"
        app:layout_constraintBottom_toBottomOf="@+id/eventos"
        app:layout_constraintStart_toEndOf="@+id/TheFlash"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239"
        app:srcCompat="@drawable/anne_poster" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="39dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:contentDescription="@string/busca"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintStart_toEndOf="@+id/logo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.451"
        app:srcCompat="@android:drawable/ic_menu_search" />

</androidx.constraintlayout.widget.ConstraintLayout>